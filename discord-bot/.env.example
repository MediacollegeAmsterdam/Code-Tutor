# Discord Bot Configuration
# Copy this file to .env and fill in your values

# Your Discord Bot Token (get from Discord Developer Portal)
DISCORD_TOKEN=your_bot_token_here

# Your Discord Application ID
CLIENT_ID=your_client_id_here

# Code Tutor Dashboard URL (default: http://localhost:51987/api/discord)
TUTOR_API_URL=http://localhost:51987/api/discord

# Guild ID for testing (optional, leave empty for global commands)
GUILD_ID=
